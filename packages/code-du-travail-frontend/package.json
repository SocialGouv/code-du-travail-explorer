{
  "name": "@cdt/frontend",
  "version": "3.2.0",
  "description": "Code du travail frontend",
  "private": true,
  "homepage": "https://code-du-travail.beta.gouv.fr/",
  "keywords": [],
  "main": "src/index.js",
  "scripts": {
    "dev": "SUGGEST_URL=http://localhost:1337/nlp/suggest node server/server.js",
    "dev-with-nlp": "node server/server.js",
    "lint": "eslint src/* pages/*",
    "prepush": "yarn lint && yarn test",
    "start": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@cdt/data": "^3.2.0",
    "@cdt/data...datafiller": "^3.2.0",
    "@cdt/data...preavis-demission": "^3.2.0",
    "@cdt/sources": "^3.2.0",
    "@cdt/ui-old": "^3.2.0",
    "@reach/tooltip": "^0.2.2",
    "@sentry/browser": "^5.6.3",
    "@socialgouv/react-fiche-service-public": "^3.2.0",
    "@zeit/next-source-maps": "^0.0.3",
    "date-fns": "^2.4.1",
    "debounce-promise": "^3.1.2",
    "express": "^4.17.1",
    "express-sitemap-xml": "^1.1.0",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "final-form-calculate": "^1.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "mdx-runtime-slim": "^1.4.5",
    "memoizee": "^0.4.14",
    "next": "^9.0.7",
    "react": "^16.10.1",
    "react-autosuggest": "9.4.3",
    "react-delay": "^0.1.0",
    "react-dom": "^16.10.1",
    "react-feather": "2.0.3",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1",
