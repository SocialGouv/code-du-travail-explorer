    "react-uid": "^2.2.0",
    "rehype-raw": "^4.0.1",
    "rehype-react": "^4.0.1",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
    "speakingurl": "14.0.1",
    "styled-components": "^4.4.0",
    "unified": "^8.4.1",
    "unist-util-find": "^1.0.1",
    "url-search-params": "^1.1.0",
    "use-persisted-state": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@socialgouv/eslint-config-react": "^0.10.0",
    "@testing-library/react": "^9.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*?(*.)+(test|spec).[jt]s?(x)"
    ],
    "collectCoverageFrom": [
      "!src/**/*mock.js",
      "src/**/*.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ]
  }
}
