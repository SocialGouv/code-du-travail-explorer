07\\"},\\"$\\":[{\\"type\\":\\"text\\",\\"$\\":\\"Agence centrale des organismes de sécurité sociale (Acoss)\\"}]}]},{\\"type\\":\\"element\\",\\"name\\":\\"Abreviation\\",\\"_\\":{\\"ID\\":\\"R24383\\",\\"type\\":\\"Sigle\\"},\\"$\\":[{\\"type\\":\\"element\\",\\"name\\":\\"Titre\\",\\"$\\":[{\\"type\\":\\"text\\",\\"$\\":\\"SARL\\"}]},{\\"type\\":\\"element\\",\\"name\\":\\"Texte\\",\\"$\\":[{\\"type\\":\\"element\\",\\"name\\":\\"Paragraphe\\",\\"$\\":[{\\"type\\":\\"text\\",\\"$\\":\\"Société à responsabilité limitée\\"}]}]}]},{\\"type\\":\\"element\\",\\"name\\":\\"Abreviation\\",\\"_\\":{\\"ID\\":\\"R31844\\",\\"type\\":\\"Acronyme\\"},\\"$\\":[{\\"type\\":\\"element\\",\\"name\\":\\"Titre\\",\\"$\\":[{\\"type\\":\\"text\\",\\"$\\":\\"Scop\\"}]},{\\"type\\":\\"element\\",\\"name\\":\\"Texte\\",\\"$\\":[{\\"type\\":\\"element\\",\\"name\\":\\"Paragraphe\\",\\"$\\":[{\\"type\\":\\"text\\",\\"$\\":\\"Société coopérative ouvrière de production\\"}]}]}]}]}",
